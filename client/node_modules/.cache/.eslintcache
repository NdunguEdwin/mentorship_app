env.REACT_APP_HOST;\n\n    const primaryLight = palette.primary.light;\n    const primaryDark = palette.primary.dark;\n    const main = palette.neutral.main;\n    const medium = palette.neutral.medium;\n\n    const isFriend = connections.find((connection) => connection._id === connectionId);\n\n    const patchFriend = async () => {\n        if(connectionId!==undefined){\n            const response = await fetch(`https://protege-mentorship-app-backend.onrender.com/users/${_id}/${connectionId}`,\n            {\n                method: \"PATCH\",\n                headers: {\n                    Authorization: `Bearer ${token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n            });\n            const data = await response.json();\n            dispatch(setFriends({ connections: data }));\n        }\n        \n    };\n\n    return (\n        <FlexBetween>\n            <FlexBetween>\n                <UserImage image={userPicturePath} size=\"55px\" />\n                <Box\n                    ml=\"0.5rem\"\n                    onClick={() => {\n                        if(connectionId!==undefined){\n                            navigate(`/profile/${connectionId}`);\n                            navigate(0);\n                        }\n                    }}\n                >\n                    <Typography\n                        color={main}\n                        variant=\"h5\"\n                        fontWeight=\"500\"\n                        sx={{\n                            \"&:hover\": {\n                                color: palette.primary.light,\n                                cursor: \"pointer\"\n                            }\n                        }}\n                    >\n                        {name}\n                    </Typography>\n                    <Typography color={medium} fontSize=\"0.75rem\">\n                        {subtitle}\n                    </Typography>\n                </Box>\n        